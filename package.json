{
  "name": "cloudcmd",
  "version": "8.5.2",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Orthodox web file manager with console and editor",
  "homepage": "http://cloudcmd.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/cloudcmd.git"
  },
  "keywords": [
    "console",
    "terminal",
    "edit",
    "editor",
    "file",
    "file manager",
    "folder",
    "orthodox",
    "view",
    "viewer",
    "copy",
    "rename",
    "move",
    "rm",
    "mv",
    "cp",
    "delete",
    "delete file",
    "delete directory",
    "remove",
    "remove file",
    "remove directory",
    "file operation",
    "pack",
    "server"
  ],
  "bin": {
    "cloudcmd": "bin/cloudcmd.js"
  },
  "config": {
    "dirs": "bin/cloudcmd.js common server",
    "dirs_test": "test bin/release.js webpack.config.js"
  },
  "scripts": {
    "start": "node bin/cloudcmd.js",
    "start:dev": "NODE_ENV=development node bin/cloudcmd.js",
    "build:start": "redrun build:client start",
    "build:start:dev": "redrun build:client:dev start:dev",
    "security": "nsp check",
    "lint": "redrun lint:*",
    "lintjs": "redrun lint:client lint:server lint:test",
    "lint:server": "eslint -c .eslintrc.server $npm_package_config_dirs",
    "lint:test": "eslint $npm_package_config_dirs_test",
    "lint:client": "eslint --env browser client",
    "lint:css": "stylelint css/*.css",
    "lint:md": "yaspeller .",
    "fix:lint:client": "redrun lint:client -- --fix",
    "fix:lint:test": "redrun lint:test -- --fix",
    "fix:lint:server": "redrun lint:server -- --fix",
    "test": "tape 'test/**/*.js'",
    "test:client": "tape 'test/client/**/*.js'",
    "wisdom": "redrun build",
    "wisdom:type": "bin/release.js",
    "docker:pull:node": "docker pull node",
    "docker:pull:alpine": "docker pull mhart/alpine-node",
    "docker:push": "docker push coderaiser/cloudcmd:`version`",
    "docker:push:latest": "docker push coderaiser/cloudcmd:latest",
    "docker:push:alpine": "docker push coderaiser/cloudcmd:`version`-alpine",
    "docker:push:alpine:latest": "docker push coderaiser/cloudcmd:latest-alpine",
    "docker:build": "docker build -t coderaiser/cloudcmd:`version` .",
    "docker:build:alpine": "docker build -f Dockerfile.alpine -t coderaiser/cloudcmd:`version`-alpine .",
    "docker": "redrun docker:pull* docker:build* docker:tag* docker:push*",
    "docker-ci": "redrun build docker-login docker",
    "docker-login": "docker login -u $DOCKER_USERNAME -p $DOCKER_PASSWORD",
    "docker:alpine": "redrun docker:pull:alpine docker:build:alpine docker:tag:alpine docker:push:alpine docker:push:alpine:latest",
    "docker:tag": "docker tag coderaiser/cloudcmd:`version` coderaiser/cloudcmd:latest",
    "docker:tag:alpine": "docker tag coderaiser/cloudcmd:`version`-alpine coderaiser/cloudcmd:latest-alpine",
    "docker:rm:version": "docker rmi -f coderaiser/cloudcmd:`version`",
    "docker:rm:latest": "docker rmi -f coderaiser/cloudcmd:latest",
    "docker:rm:alpine": "docker rmi -f coderaiser/cloudcmd:`version`-alpine",
    "docker:rm:latest-alpine": "docker rmi -f coderaiser/cloudcmd:latest-alpine",
    "docker:rm-old": "redrun -P docker:rm:*",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "6to5": "webpack --progress",
    "6to5:client": "npm run 6to5 -- --mode production",
    "6to5:client:dev": "NODE_ENV=development npm run 6to5 -- --progress --mode development",
    "pre6to5:client": "rimraf dist",
    "pre6to5:client:dev": "rimraf dist-dev",
    "watch:client": "redrun 6to5:client -- --watch",
    "watch:client:dev": "redrun 6to5:client:dev -- --watch",
    "watch:server": "nodemon bin/cloudcmd.js",
    "watch:lint": "nodemon -w client -w server -w webpack.config.js -x 'redrun lint:js'",
    "watch:lint:client": "nodemon -w client -w webpack.config.js -x 'redrun lint:js:eslint:client'",
    "watch:test": "nodemon -w server -w test -w common -x \"npm test\"",
    "watch:test:client": "nodemon -w client -w test/client -x \"npm run test:client\"",
    "watch:coverage": "nodemon -w server -w test -w common -x \"npm run coverage\"",
    "w:c": "redrun watch:client",
    "w:c:d": "redrun watch:client:dev",
    "build": "redrun 6to5:*",
    "build:client": "redrun 6to5:client",
    "build:client:dev": "redrun 6to5:client:dev",
    "heroku-postbuild": "redrun 6to5:client",
    "postheroku-postbuild": "npm i gritty"
  },
  "directories": {
    "man": "man"
  },
  "subdomain": "cloudcmd",
  "dependencies": {
    "@cloudcmd/fileop": "^1.0.1",
    "@cloudcmd/read-files-sync": "^1.0.0",
    "apart": "^1.0.1",
    "chalk": "^2.0.1",
    "checkup": "^1.3.0",
    "console-io": "^7.0.0",
    "copymitter": "^2.0.0",
    "criton": "^1.0.0",
    "currify": "^2.0.3",
    "deepmerge": "^2.0.0",
    "deepword": "^3.0.0",
    "dword": "^7.0.0",
    "edward": "^7.0.0",
    "es6-promisify": "^6.0.0",
    "execon": "^1.2.0",
    "express": "^4.13.0",
    "files-io": "^1.2.0",
    "flop": "^4.0.0",
    "format-io": "^0.9.6",
    "fullstore": "^1.0.0",
    "http-auth": "^3.2.3",
    "inly": "^1.0.2",
    "jaguar": "^3.0.0",
    "jju": "^1.3.0",
    "join-io": "^2.0.0",
    "jonny": "^2.0.3",
    "markdown-it": "^8.0.0",
    "mellow": "^2.0.0",
    "minimist": "^1.2.0",
    "nomine": "^1.0.1",
    "onezip": "^1.0.5",
    "opn": "^5.1.0",
    "package-json": "^4.0.1",
    "ponse": "^1.4.0",
    "pullout": "^1.0.1",
    "rendy": "^1.1.0",
    "restafary": "^3.0.0",
    "socket.io": "^2.0.3",
    "squad": "^2.0.0",
    "table": "^4.0.1",
    "try-catch": "^2.0.0",
    "tryrequire": "^2.0.1",
    "wraptile": "^1.0.0",
    "writejson": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "clean-css-loader": "^0.1.2",
    "clear-module": "^2.1.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.4",
    "emitify": "^3.0.2",
    "es6-promise": "^4.0.5",
    "eslint": "^4.0.0",
    "eslint-plugin-node": "^6.0.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.4",
    "fresh-require": "^1.0.3",
    "gunzip-maybe": "^1.3.1",
    "html-looks-like": "^1.0.2",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "html-webpack-plugin": "^3.0.7",
    "inherits": "^2.0.3",
    "limier": "^2.0.0",
    "minor": "^1.2.2",
    "mkdirp": "^0.5.1",
    "mock-require": "^3.0.1",
    "morgan": "^1.6.1",
    "nodemon": "^1.9.1",
    "nsp": "^3.1.0",
    "nyc": "^11.0.2",
    "philip": "^2.0.0",
    "place": "^1.1.4",
    "readjson": "^1.1.3",
    "redrun": "^6.0.0",
    "request": "^2.76.0",
    "rimraf": "^2.5.4",
    "shortdate": "^1.0.1",
    "sinon": "^4.0.0",
    "sinon-called-with-diff": "^2.0.0",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.20.1",
    "stylelint": "^9.0.0",
    "stylelint-config-standard": "^18.0.0",
    "tape": "^4.4.0",
    "tar-stream": "^1.5.2",
    "url-loader": "^1.0.1",
    "version-io": "^2.0.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "yaspeller": "^4.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "main": "server/cloudcmd.js"
}
